<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="menu1">
        <a href="~/Home/Index">Home</a>
        <a href="~/Member/Index">Members Only</a>
        <a href="~/Game/Index">PLay Crosswords</a>
        <a href="~/Puzzle/Index">Puzzles</a>
        <a href="~/Clue/Index">Add a Puzzle</a>

    </div>
    <div class="menu2">
        <a href="~/Home/History">History</a>
        <a href="~/Home/About">About</a>
        <a href="~/Home/Contact">Contact</a>
    </div>
    <div class="container body-content">
        <h1>
            @ViewBag.Title
        </h1>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - GEPW Crossword website</p>
        </footer>
    </div>
</body>
</html>
